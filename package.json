{
  "name": "relity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:startup": "yarn cdn:dev && yarn cdn:http && yarn dev:wss && yarn dev:next",
    "cdn:http": "cd ./src/fileserver/cdn && http-server -p 4200 --cors",
    "cdn:start": "node ./src/fileserver/server.js",
    "cdn:dev": "nodemon ./src/fileserver/server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:start": "docker compose up -d",
    "db:push": "npx prisma db push",
    "db:stop": "docker compose down",
    "build": "next build",
    "dev:next": "next dev",
    "dev:wss": "cross-env PORT=3001 NODE_ENV=development NEXTAUTH_URL=http://localhost:3000/api/v1/auth tsx watch src/server/trpc/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "4.7.1",
    "@tanstack/react-query": "^4.10.0",
    "@trpc/client": "10.0.0-rc.4",
    "@trpc/next": "10.0.0-rc.4",
    "@trpc/react-query": "10.0.0-rc.4",
    "@trpc/server": "10.0.0-rc.4",
    "@types/crypto-js": "^4.1.1",
    "axios": "^1.2.0",
    "cors": "^2.8.5",
    "cropperjs": "^1.5.13",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "node-fetch": "^3.3.0",
    "nodemon": "^2.0.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.56.1",
    "superjson": "1.9.1",
    "tsx": "^3.12.1",
    "ws": "^8.11.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.2",
    "prisma": "4.7.1",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.10.0"
  }
}
