    "axios": "^1.2.0",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "node-fetch": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.56.1",
